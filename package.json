    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@ng-bootstrap/ng-bootstrap": "11.0.0",
    "@prettier/plugin-pug": "1.19.0",
    "change-case": "4.1.2",
    "chart.js": "3.6.2",
